.itemBox{
    width:100%;
    height:234rpx;
}
.class-container .wrap {
  border-top: 1px #f0f0f0 solid;
  padding: 24rpx;
}
.class-container .wrap image {
  width: 240rpx;
  height: 178rpx;
  margin-right: 28rpx;
  float: left;
}
.class-container .wrap .info .title {
  color: #313133;
  font-size: 30rpx;
}
.class-container .wrap .info .type {
  color: #abacb2;
  font-size: 24rpx;
  display: block;
}
.class-container .wrap .info .current-price {
  color: #ff7f4d;
  font-size: 32rpx;
  font-weight: bold;
}
.class-container .wrap .info .origin-price {
  color: #c4c5cc;
  font-size: 26rpx;
  text-decoration:line-through;
  margin-left: 16rpx;
}
.class-container .wrap .info .num{
  font-size:26rpx;
  color:#313133;
  float:right;
}
