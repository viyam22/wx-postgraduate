.onlinetest{
  height: 100%;
  background-color: #fff;
}
.test{
  display: flex;
  background-color: #fff;
  padding: 40rpx 65rpx;
  /* justify-content:space-between; */
  flex-flow: row wrap;
  align-content: flex-start;
}
.num{
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 50%;
  border: 2rpx solid #b2b2b2;
  text-align: center;
  color: #939799;
  font-size: 30rpx;
  margin: 10rpx 15rpx;
}
.selected{
  background-color: #0099ff;
  color: #fff;
  border: 2rpx solid #0099ff;
}
.btn{
    width:690rpx;
    box-sizing: border-box;
	  background-image: linear-gradient(0deg, #0099ff 0%, #0ddaff 100%);
	  border-radius: 10rpx;
    color: #fff;
    position: absolute;
    bottom: 20rpx;
    left: 50%;
    transform: translateX(-50%);
}