<view class="rank-lists">
  <view class="title">成绩排名</view>
  <view class="rank-table">
    <view class="header">
      <view class="td-s">排名</view>
      <view class="td-l">姓名</view>
      <view class="td-s">成绩</view>
    </view>
    <view class="lists">
      <view class="item" wx:for="{{rankData}}" wx:key="index">
        <view class="td-s td-blue-sel">{{item.rank}}</view>
        <view class="td-l td-blue">{{item.name}}</view>
        <view class="td-s td-blue-sel">{{item.score}}</view>
      </view>
    </view>
  </view>
</view>